		<script>
			(async function(window) {
				'use strict';

				const options = {
					localPath: "/content/serviceWorker.js",
					localScope: "/content/logitechg",
					dispatcherScope: "/",
					dispatcherPath: "/sw-gaming.js"
				};

				const isDispatcher = () => {
					return !location.origin.match(/4502|4503|author|publish/);
				}

				const useServiceWorkers = ('serviceWorker' in navigator);

				/**
				 * Service Worker
				 *
				 * @summary
				 * Creates a service worker based on the parameters passed in.
				 * Listens for messages from the service worker.
				 * Creates an optional kill switch to unregister all service workers.
				 *
				 * @alias ServiceWorker
				 *
				 * @example
					if (ServiceWorker.useServiceWorkers) {
						const serviceWorker = new ServiceWorker(options);
						if (serviceWorker.killServiceWorkers) {
							serviceWorker.sendKillCache();
							serviceWorker.killSwitch();
						} else {
							await serviceWorker.init(serviceWorker.getOptions());
						}
					}
				 */
				class ServiceWorker {

					/**
					 *
					 * @param {Object} options Paths and scopes of service workers
					 */
					constructor(options) {
						this.options = options;
						this.isOnline = ('onLine' in navigator) && navigator.onLine;
						this.serviceWorkerRegistration;
						this.killServiceWorkers = false; // Could be overwritten with an option in AEM's page properties.
						
						if (!this.killServiceWorkers) {
							this.attachListeners();
						}
					}

					getOptions() {
						let path;
						let scope;
						const {dispatcherPath, dispatcherScope, localPath, localScope} = this.options;

						if (isDispatcher()) {
							path = dispatcherPath;
							scope = dispatcherScope
						} else {
							path = localPath;
							scope = localScope;
						}

						return {
							path,
							scope
						};
					}

					/**
					 * Initializes a service worker and gets a status update.
					 *
					 * @param {Object} options path and scope options
					 */
					async init(options) {
						try {
							this.serviceWorkerRegistration = await navigator.serviceWorker.register(options.path, {
								scope: options.scope,
								updateViaCache: 'none', // Never consult the HTTP cache
							});

							// What state is the service worker in? It could be one of these three.
							this.serviceWorker = this.serviceWorkerRegistration.installing || // First time service worker has been loaded
											/** 
											 * Only have one service worker can be active at a time, if one is already active then the state is waiting.
											 * The user has to navigate to a whole new page before the new service worker can load. This can be
											 * skipped with self.skipWaiting() in the service worker. 
											 */
											this.serviceWorkerRegistration.waiting ||
											this.serviceWorkerRegistration.active; // Service worker is installed and active.
							
							this.sendStatusUpdate(this.serviceWorker);
							
							// Listern for new service worker to take over when controllerchange
							navigator.serviceWorker.addEventListener('controllerchange', () => {
								this.serviceWorker = navigator.serviceWorker.controller;
								this.sendStatusUpdate(this.serviceWorker);
							});

							// Listen for messages from the service worker
							navigator.serviceWorker.addEventListener('message', this.onServiceWorkerMessage.bind(this));

						} catch (err) {
							console.warn(`Init Service Worker error: `);
						}
					}

					attachListeners() {
						window.addEventListener('online', () => {
							this.isOnline = true;
							this.sendStatusUpdate();
						});

						window.addEventListener('offline', () => {
							this.isOnline = false;
							this.sendStatusUpdate();
						});
					}

					/**
					 * Event listener callback for message event
					 *
					 * @param {Object} evt Event object from message event
					 */
					onServiceWorkerMessage(evt) {
						const {data} = evt;
						if (data.requestStatusUpdate) {
							console.log('Received status update request from service worker, responding...');
							// Send message on message channel target.
							this.sendStatusUpdate(evt.ports && evt.ports[0]);
						}
					}

					sendStatusUpdate(target) {
						this.sendServiceWorkerMessage({statusUpdate: {isOnline: this.isOnline}}, target);
					}

					sendKillCache() {
						this.sendServiceWorkerMessage({killCache: true});
					}

					/**
					 * Sends a message to an optional target or current service worker.
					 *
					 * @param {Object|String} msg Message to send to service worker
					 * @param {Object} [target] Optional target to post the message
					 */
					sendServiceWorkerMessage(msg, target) {
						if (target) {
							target.postMessage(msg);
						} else if (this.serviceWorker) {
							this.serviceWorker.postMessage(msg);
						} else { // If first 2 conditions fail, fallback to the service worker controller
							navigator.serviceWorker.controller.postMessage(msg);
						}
					}

					killSwitch() {
						navigator.serviceWorker.getRegistrations().then(registrations => {
							for (let registration of registrations) {
								registration.unregister();
							}
						});
					}
				}

				// Create the service worker
				if (useServiceWorkers) {
					const serviceWorker = new ServiceWorker(options);
					if (serviceWorker.killServiceWorkers) {
						serviceWorker.sendKillCache();
						serviceWorker.killSwitch();
					} else {
						await serviceWorker.init(serviceWorker.getOptions());
					}
				}
			})(window);
		</script>
	


    
  
      
  


    

    
  	
